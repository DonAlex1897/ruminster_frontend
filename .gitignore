# .gitignore for ASP.NET Core / .NET projects
# Build results
bin/
obj/

# User-specific files
*.user
*.suo
*.userosscache
*.sln.docstates

# Visual Studio Code
.vscode/

# Rider
.idea/

# OS generated files
.DS_Store
Thumbs.db

# Logs
*.log

# NuGet packages
*.nupkg

# Flutter/Dart/Pub related
mobile_app/**/doc/api/
mobile_app/.dart_tool/
mobile_app/.flutter-plugins
mobile_app/.flutter-plugins-dependencies
mobile_app/.packages
mobile_app/.pub-cache/
mobile_app/.pub/
mobile_app/build/

# Android related
mobile_app/android/**/gradle-wrapper.jar
mobile_app/android/.gradle/
mobile_app/android/captures/
mobile_app/android/gradlew
mobile_app/android/gradlew.bat
mobile_app/android/local.properties
mobile_app/android/**/GeneratedPluginRegistrant.java

# iOS related
mobile_app/ios/Flutter/Generated.xcconfig
mobile_app/ios/Flutter/flutter_export_environment.sh
mobile_app/ios/Flutter/App.framework
mobile_app/ios/Flutter/Flutter.framework
mobile_app/ios/Flutter/Flutter.podspec
mobile_app/ios/Pods/
mobile_app/ios/.symlinks/
mobile_app/ios/Flutter/app.flx
mobile_app/ios/Flutter/app.zip
# NuGet restore
.nuget/

# Others
*.vspscc
*.vssscc

# Publish results
publish/

# Static web assets
**/staticwebassets/

# Temporary files
*.tmp
*.temp

# Test results
TestResults/

# Coverage reports
coverage/
*.coverage
*.coveragexml

# Backup & crash reports
*.bak
*.crash

# Resharper
_ReSharper*/
*.[Rr]e[Ss]harper

# DotCover
*.dotCover

# Local secrets
secrets.json

# App settings (contains secrets and environment-specific config)
appsettings.json
appsettings.Development.json
